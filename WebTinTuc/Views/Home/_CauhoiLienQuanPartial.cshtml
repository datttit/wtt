@model List<WebTinTuc.Models.HoiDapCauHois>
@if (Model != null)
{
    if (Model.Count > 0)
    {
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="@string.Format("/hoi-dap/cau-hoi/{0}", item.CauHoiId)">@item.TenCauHoi</a></li>
            }
        </ul>
    }
}
else
{
    <span>Chưa có câu hỏi.</span>
}
