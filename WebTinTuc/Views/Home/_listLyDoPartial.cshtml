@model List<WebTinTuc.Models.Lydo>

@if (Model != null)
{
    if (Model.Count > 0)
    {

        foreach (var item in Model)
        {
            <div class="checkbox">
                <label>
                    <input type="checkbox" name="LydoId" value="@item.LyDoId" /> @item.TenLyDo
                </label>
            </div>
        }
    }
}
